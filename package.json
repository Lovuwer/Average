{
  "name": "Average",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:backend": "cd backend && npx jest --config jest.config.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='__tests__/unit'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:components": "jest --testPathPattern='__tests__/components'",
    "test:screens": "jest --testPathPattern='__tests__/screens'",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "build:verify:android": "bash __tests__/build/android-build-verification.sh",
    "build:verify:ios": "bash __tests__/build/ios-build-verification.sh",
    "build:verify:config": "npx ts-node __tests__/build/verify-native-config.ts",
    "build:fix:android": "bash scripts/fix-android-build.sh",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:ios:release": "cd ios && xcodebuild -workspace Average.xcworkspace -scheme Average -configuration Release -sdk iphoneos archive",
    "lint": "eslint src/ --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-community/blur": "^4.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.1.1",
    "@shopify/react-native-skia": "^1.5.0",
    "axios": "^1.13.5",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "react-native-device-info": "^13.2.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-permissions": "^5.2.3",
    "react-native-reanimated": "3.16.7",
    "react-native-safe-area-context": "^5.1.0",
    "react-native-screens": "4.4.0",
    "react-native-sqlite-storage": "^6.0.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.3.0",
    "@react-native-community/cli": "^20.1.1",
    "@react-native-community/cli-platform-android": "^20.1.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.18",
    "@types/react-native": "^0.73.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "react-test-renderer": "^18.3.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.7.3",
    "zod": "^3.24.1"
  }
}
